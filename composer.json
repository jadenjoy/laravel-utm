{
    "name": "adzbuck/laravel-utm",
    "description": "Keeps track of the UTM parameters",
    "keywords": [
        "spatie",
        "adzbuck",
        "laravel-utm"
    ],
    "homepage": "https://github.com/adzbuck/laravel-utm",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Vanderbist",
            "email": "alex.vanderbist@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Adam Buckley",
            "email": "adzbuck@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.16",
        "orchestra/testbench": "^6.27|^7.9|^8.4",
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.9"
    },
    "autoload": {
        "psr-4": {
            "Adzbuck\\LaravelUTM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adzbuck\\LaravelUTM\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "Adzbuck\\LaravelUTM\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
